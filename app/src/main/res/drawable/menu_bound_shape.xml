<?xml version="1.0" encoding="utf-8"?>
<!-- 定义矩形rectangle -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"  >

    <!-- 设置边框的大小和颜色 -->
    <stroke android:width="5dip" />

    <!-- 设置矩形内的颜色,此处为透明色 -->
    <solid android:color="#674E26"/>

    <!-- 定义圆角弧度 -->
    <corners android:radius="0dip"/>

</shape>